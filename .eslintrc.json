{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": [
    "plugin:react/recommended"
  ],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "react/prop-types": [
      "error",
      {
        "ignore": [
          "navigation",
          "route"
        ]
      }
    ],
    "semi": "warn",
    "eol-last": "warn",
    "no-undef": "warn",
    "no-console": "warn",
    "no-multi-spaces": "warn",
    "no-trailing-spaces": "warn",
    "comma-dangle": [
      "warn",
      "always-multiline"
    ],
    "quotes": [
      "warn",
      "double"
    ],
    "comma-style": [
      "warn",
      "last"
    ],
    "no-multiple-empty-lines": [
      "warn",
      {
        "max": 1,
        "maxEOF": 0,
        "maxBOF": 0
      }
    ],
    "indent": [
      "warn",
      2,
      {
        "SwitchCase": 1
      }
    ]
  }
}
